{
    "name": "enflow/redirect-pizza-php-sdk",
    "description": "An SDK to easily work with the redirect.pizza API",
    "keywords": [
        "redirect.pizza",
        "api",
        "sdk"
    ],
    "homepage": "https://github.com/enflow/redirect-pizza-php-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Michel Bardelmeijer",
            "email": "michel@enflow.nl"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.5.1",
        "friendsofphp/php-cs-fixer": "^3.3"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "RedirectPizza\\PhpSdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RedirectPizza\\PhpSdk\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    }
}
